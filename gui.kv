            pos_hint: {'top': 0.92, 'x': 0.01}
            
            ScrollView:
                size_hint: 0.98, 1
                pos_hint: {'top': 1, 'x': 0.02}
                do_scroll_x: False
                do_scroll_y: True
                bar_width: 4
                bar_color: 0, 0, 0, 1

